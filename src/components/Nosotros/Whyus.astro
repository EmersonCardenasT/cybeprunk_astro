<style>
  :root {
    --steel:    #4a6278;
    --steel-lt: #7a98ae;
    --olive:    #6b7c55;
    --olive-lt: #8fa070;
    --white:    #f4f3ef;
    --black:    #0a0a0a;
  }

  .why-section {
    --bg:      #f4f3ef; --bg-card: #ffffff;
    --text:    #0a0a0a; --muted:   rgba(10,10,10,0.42);
    --border:  rgba(10,10,10,0.08);
    background: var(--bg); color: var(--text);
    position: relative; padding: 120px 0 100px;
    font-family: 'DM Sans', 'Helvetica Neue', sans-serif;
    overflow: hidden; transition: background 0.4s, color 0.4s;
  }
  :root.dark .why-section {
    --bg: #0c0c0c; --bg-card: #111111;
    --text: #f4f3ef; --muted: rgba(244,243,239,0.38);
    --border: rgba(244,243,239,0.07);
  }
  :root.dark .title-outline { -webkit-text-stroke: 1px rgba(244,243,239,0.28) !important; }

  /* BG */
  .why-bg { position: absolute; inset: 0; pointer-events: none; }
  .why-grain {
    position: absolute; inset: 0; opacity: 0.025;
    background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 512 512' xmlns='http://www.w3.org/2000/svg'%3E%3Cfilter id='g'%3E%3CfeTurbulence type='fractalNoise' baseFrequency='0.75' numOctaves='4' stitchTiles='stitch'/%3E%3C/filter%3E%3Crect width='100%25' height='100%25' filter='url(%23g)'/%3E%3C/svg%3E");
    background-size: 256px;
  }
  .why-line { position: absolute; left: 0; right: 0; height: 1px; background: var(--border); }
  .why-line-top { top: 0; } .why-line-bottom { bottom: 0; }
  .why-watermark {
    position: absolute; left: -20px; bottom: 60px;
    font-family: 'DM Serif Display', serif;
    font-size: clamp(5rem, 11vw, 9rem);
    font-weight: 400; color: transparent;
    -webkit-text-stroke: 1px var(--border);
    letter-spacing: -0.02em; user-select: none;
    opacity: 0.5; white-space: nowrap; line-height: 1;
  }

  /* Inner */
  .why-inner {
    max-width: 1280px; margin: 0 auto; padding: 0 44px;
    display: flex; flex-direction: column; gap: 64px;
    position: relative; z-index: 1;
  }
  @media (max-width: 640px) { .why-inner { padding: 0 20px; gap: 48px; } }

  /* Header */
  .why-header { display: flex; flex-direction: column; gap: 16px; }
  .why-eyebrow {
    display: flex; align-items: center; gap: 10px;
    font-size: 0.65rem; letter-spacing: 0.26em;
    text-transform: uppercase; color: var(--steel-lt); margin: 0;
  }
  .eyebrow-pip {
    width: 7px; height: 7px; background: var(--olive-lt);
    border-radius: 50%; box-shadow: 0 0 8px var(--olive); flex-shrink: 0;
  }
  .why-header-row {
    display: flex; align-items: flex-end;
    justify-content: space-between; gap: 40px; flex-wrap: wrap;
  }
  .why-title {
    font-family: 'DM Serif Display', Georgia, serif;
    font-size: clamp(2.4rem, 5vw, 4.2rem);
    font-weight: 400; line-height: 0.93;
    letter-spacing: -0.035em; color: var(--text); margin: 0;
  }
  .title-outline { color: transparent; -webkit-text-stroke: 1px rgba(10,10,10,0.35); }
  .why-sub {
    font-size: 0.88rem; color: var(--muted); line-height: 1.75;
    margin: 0; font-weight: 300; max-width: 380px; flex-shrink: 0;
  }

  /* ══ BODY ══ */
  .why-body {
    display: grid;
    grid-template-columns: 380px 1fr;
    gap: 72px; align-items: start;
  }
  @media (max-width: 1000px) { .why-body { grid-template-columns: 1fr; gap: 48px; } }

  /* ── VISUAL ── */
  .why-visual { position: relative; }
  .why-img-wrap {
    position: relative; overflow: hidden;
    aspect-ratio: 3/4;
  }
  @media (max-width: 1000px) { .why-img-wrap { aspect-ratio: 16/7; } }

  .why-img {
    width: 100%; height: 100%; object-fit: cover; object-position: center top;
    display: block;
    transition: transform 0.7s cubic-bezier(0.22,1,0.36,1);
  }
  .why-visual:hover .why-img { transform: scale(1.04); }
  .why-img-overlay {
    position: absolute; inset: 0;
    background: linear-gradient(to top, rgba(10,10,10,0.6) 0%, transparent 55%);
  }

  /* Float cards */
  .why-float {
    position: absolute;
    background: var(--bg-card);
    border: 1px solid var(--border);
    padding: 14px 18px;
    display: flex; flex-direction: column; gap: 3px;
    backdrop-filter: blur(8px);
    -webkit-backdrop-filter: blur(8px);
    transition: transform 0.3s;
  }
  .why-float:hover { transform: translateY(-3px); }
  .why-float-top {
    top: 24px; right: -20px;
  }
  .why-float-bottom {
    bottom: 28px; left: -20px;
  }
  @media (max-width: 1000px) {
    .why-float-top  { right: 20px; top: 20px; }
    .why-float-bottom { left: 20px; bottom: 20px; }
  }

  .why-float-stars {
    display: flex; gap: 2px; color: var(--olive-lt);
    margin-bottom: 2px;
  }
  .why-float-num {
    font-family: 'DM Serif Display', serif;
    font-size: 1.6rem; font-weight: 400;
    color: var(--text); letter-spacing: -0.04em; line-height: 1;
  }
  .why-float-num-sm { font-size: 1.3rem; }
  .why-float-lbl {
    font-size: 0.58rem; letter-spacing: 0.18em;
    text-transform: uppercase; color: var(--muted); font-weight: 400;
  }

  /* ── REASONS LIST ── */
  .why-reasons { display: flex; flex-direction: column; }

  .why-reason {
    display: grid;
    grid-template-columns: 52px 1fr;
    gap: 0 20px;
    padding: 0 0 32px;
    position: relative;
  }
  .why-reason-last { padding-bottom: 0; }

  .why-reason-left {
    display: flex; flex-direction: column; align-items: center; gap: 0;
  }
  .why-reason-icon {
    width: 44px; height: 44px; flex-shrink: 0;
    display: flex; align-items: center; justify-content: center;
    border: 1px solid var(--border);
    color: var(--steel);
    background: var(--bg-card);
    z-index: 1; position: relative;
    transition: border-color 0.3s, color 0.3s, box-shadow 0.3s, background 0.3s;
  }
  .why-reason:hover .why-reason-icon {
    border-color: var(--steel);
    color: var(--steel-lt);
    background: rgba(74,98,120,0.05);
    box-shadow: 0 0 16px rgba(74,98,120,0.15);
  }

  .why-reason-line {
    flex: 1; width: 1px;
    background: var(--border);
    margin-top: 0;
    min-height: 20px;
  }

  .why-reason-content {
    display: flex; flex-direction: column; gap: 10px;
    padding-top: 10px;
  }

  .why-reason-header {
    display: flex; align-items: center; gap: 12px; flex-wrap: wrap;
  }
  .why-reason-title {
    font-family: 'DM Serif Display', serif;
    font-size: 1.15rem; font-weight: 400;
    letter-spacing: -0.02em; color: var(--text); margin: 0;
    line-height: 1.2;
    transition: color 0.25s;
  }
  .why-reason:hover .why-reason-title { color: var(--steel-lt); }

  .why-reason-tag {
    font-size: 0.52rem; font-weight: 700;
    letter-spacing: 0.2em; text-transform: uppercase;
    color: var(--steel-lt);
    background: rgba(74,98,120,0.08);
    border: 1px solid rgba(74,98,120,0.22);
    padding: 3px 9px; flex-shrink: 0;
  }

  .why-reason-desc {
    font-size: 0.82rem; color: var(--muted);
    line-height: 1.75; margin: 0; font-weight: 300;
  }

  .why-reason-proof {
    display: flex; align-items: center; gap: 7px;
    font-size: 0.65rem; color: var(--olive-lt);
    letter-spacing: 0.05em; font-weight: 400;
  }
  .why-reason-proof svg { flex-shrink: 0; }

  /* ══ TESTIMONIALS ══ */
  .why-testimonials { display: flex; flex-direction: column; gap: 24px; }
  .why-test-label {
    display: flex; align-items: center; gap: 10px;
    font-size: 0.65rem; letter-spacing: 0.26em;
    text-transform: uppercase; color: var(--steel-lt); margin: 0;
  }

  .why-test-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 1px;
    background: var(--border);
    border: 1px solid var(--border);
  }
  @media (max-width: 860px) { .why-test-grid { grid-template-columns: 1fr; } }

  .why-test-card {
    background: var(--bg-card);
    padding: 28px;
    display: flex; flex-direction: column; gap: 16px;
    transition: background 0.25s;
    position: relative; overflow: hidden;
  }
  .why-test-card::after {
    content: '';
    position: absolute; bottom: 0; left: 0; right: 0; height: 2px;
    background: linear-gradient(to right, var(--olive), var(--steel));
    transform: scaleX(0); transform-origin: left;
    transition: transform 0.4s cubic-bezier(0.4,0,0.2,1);
  }
  .why-test-card:hover { background: color-mix(in srgb, var(--bg-card), var(--steel) 3%); }
  :root.dark .why-test-card:hover { background: rgba(74,98,120,0.05); }
  .why-test-card:hover::after { transform: scaleX(1); }

  .why-test-stars { display: flex; gap: 3px; color: var(--olive-lt); }

  .why-test-text {
    font-size: 0.82rem; color: var(--text);
    line-height: 1.75; margin: 0; font-weight: 300;
    font-style: italic;
    flex: 1;
  }

  .why-test-author {
    display: flex; align-items: center; gap: 12px;
    padding-top: 12px;
